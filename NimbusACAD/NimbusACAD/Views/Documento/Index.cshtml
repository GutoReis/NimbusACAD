@model IEnumerable<NimbusACAD.Models.DB.Negocio_Documento>

@{
    ViewBag.Title = "Index";
}

<h2>Documento</h2>
<br />
<p>
    @Html.ActionLink("Novo Documento", "NovoDocumento")
</p>

<table class="table">
    <tr>
        <th>
            Nome do documento
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Documento_Nome)
        </td>
        <td>
            @Html.ActionLink("Detalhes", "Detalhes", new { id=item.Documento_ID })
        </td>
    </tr>
}

</table>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

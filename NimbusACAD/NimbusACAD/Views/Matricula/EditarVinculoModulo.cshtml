@model NimbusACAD.Models.DB.Negocio_Vinculo_Modulo

@{
    ViewBag.Title = "EditarVinculoModulo";
}

<h2>EditarVinculoModulo</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
